{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:vue/vue3-essential",
    "plugin:tailwind/recommended"
  ],
  "rules": {
    "prettier/prettier": ["error"]
  }
}
